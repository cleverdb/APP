.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.img-area{
  display: flex;
  height: auto;
  flex-wrap:wrap;
  column-count: 4;

}
.upload-image{
  width: 170rpx;
  height: 175rpx;
  margin: 0 10rpx 10rpx 5rpx;
  border-radius: 5px;
}

